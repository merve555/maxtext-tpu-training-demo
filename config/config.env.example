# MaxText TPU Training Demo Configuration
# Copy this file to config.env and update with your actual values

# Google Cloud Configuration
PROJECT_ID=your-project-id-here
REGION=us-east5
CLUSTER_NAME=maxtext-tpu-cluster

# Artifact Registry Configuration
REPO_NAME=maxtext-demo
DOCKER_REGISTRY_URL=us-east5-docker.pkg.dev

# HuggingFace Configuration
HUGGINGFACE_TOKEN=hf_your_token_here

# GCS Bucket Configuration (will be auto-generated based on PROJECT_ID)
ARTIFACTS_BUCKET_SUFFIX=maxtext-tpu-training-demo-artifacts
DATASETS_BUCKET_SUFFIX=maxtext-tpu-training-demo-datasets

# Model Configuration
MODEL_NAME=gemma2-2b
TOKENIZER_PATH=google/gemma-2-2b-it
CHECKPOINT_ID=20250928-190725

# Training Configuration
OUTPUT_DIR=/gcs/artifacts/output/finetuned
SCANNED_CKPT_BASE_DIR=/gcs/artifacts/checkpoints/scanned
HF_OUTPUT_BASE_DIR=/gcs/artifacts/finetuned-hf-models

# Docker Image Tags
MAXTEXT_IMAGE_TAG=jax0.7.2-rev1
VLLM_IMAGE_TAG=latest
